<div class="mb-3">
    <label for="<%= id %>" class="form-label"><%= label %></label>
    <input
    type="<%= type %>"
    class="form-control"
    id="<%= id %>"
    name="<%= name %>"
    placeholder="<%= placeholder || '' %>"
    <%= if (typeof min !== undefined) { %>min="<%= min %>"<% } %>
    <%= if (typeof max !== undefined) { %>max="<%= max %>"<% } %>
    <%= if (required) { %>required<%} %>
    >
</div>